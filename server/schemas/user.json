{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "emailAddress": {
            "type": "string",
            "format": "email"
        },
        "password": {
            "type": "string",
            "minLength": 6
        },
        "firstName": {
            "type": "string",
            "minLength": 1
        },
        "lastName": {
            "type": "string",
            "minLength": 1
        },
        "dateOfBirth": {
            "type": "string",
            "format": "date"
        }
    },
    "required": [
        "emailAddress",
        "password",
        "firstName",
        "lastName",
        "dateOfBirth"
    ],
    "additionalProperties": false
}